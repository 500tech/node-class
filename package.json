{
  "name": "node-class",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "_moduleAliases": {
    "@root": ".",
    "@services": "src/services",
    "@app": "src/app",
    "@mw": "src/middleware"
  },
  "scripts": {
    "tape-integration": "rimraf db.test.json && tape tests/integration/**/*.js;fkill -s :4000",
    "test-unit": "nyc --silent tape -r module-alias/register tests/unit/**/*.js",
    "test-app": "node index.js",
    "start-test": "nyc --no-clean --silent npm run test-app",
    "start": "cross-env NODE_ENV=dev nodemon index.js",
    "start-prod": "cross-env NODE_ENV=production node index.js",
    "test-integration": "cross-env NODE_ENV=test concurrently 'npm:start-test' 'npm:tape-integration'",
    "test-all": "npm run test-unit && npm run test-integration",
    "test": "npm run test-all && nyc report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/500tech/node-class.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/500tech/node-class/issues"
  },
  "homepage": "https://github.com/500tech/node-class#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.1.0",
    "fkill-cli": "^5.2.0",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "module-alias": "^2.2.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "proxyquire": "^2.1.0",
    "pug": "^2.0.4",
    "redis": "^2.8.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "detect-port": "^1.3.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "sleep-promise": "^8.0.1",
    "tape": "^4.11.0",
    "xyquest": "^2.1.1"
  },
  "nyc": {
    "exclude": [
      "tests/**/*.js"
    ]
  }
}
